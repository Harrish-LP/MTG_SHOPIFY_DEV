function getUKTime(date) {
  // Create a UK-specific date formatter with Europe/London timezone
  const formatter = new Intl.DateTimeFormat('en-GB', {
    timeZone: 'Europe/London',  // This handles BST/GMT transitions automatically
    hour12: false,
    year: 'numeric',
    month: '2-digit',
    day: '2-digit',
    hour: '2-digit',
    minute: '2-digit',
    second: '2-digit'
  });

  // Get the formatted date string
  const formattedDate = formatter.format(date);
  const [datePart, timePart] = formattedDate.split(', ');
  const [day, month, year] = datePart.split('/');
  const [hour, minute, second] = timePart.split(':');

  const ukDate = new Date(year, month - 1, day, hour, minute, second);

  const isBST = formatter.resolvedOptions().timeZone === 'Europe/London' &&
    date.getTimezoneOffset() !== 0;

  return ukDate;
}

function updateEventCards() {
  const now = getUKTime(new Date());
  const cards = document.querySelectorAll('.card-helen-webinar');

  cards.forEach(card => {
    const endUTC = new Date(card.getAttribute("data-end-helen"));
    const end = getUKTime(endUTC);
    const registerBtn = card.querySelector(".webinar-register-btn");
    const badge = card.querySelector(".badge-helen");

    const diffEnd = end - now;

    if (diffEnd > 0) {
      // Calculate days, hours, minutes, and seconds
      const days = Math.floor(diffEnd / (1000 * 60 * 60 * 24));
      const hours = Math.floor((diffEnd % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
      const minutes = Math.floor((diffEnd % (1000 * 60 * 60)) / (1000 * 60));
      const seconds = Math.floor((diffEnd % (1000 * 60)) / 1000);

      // Build the time remaining string
      let timeLeft = '';
      if (days > 0) {
        timeLeft = `${days}d ${hours}h ${minutes}m ${seconds}s left`;
      } else if (hours > 0) {
        timeLeft = `${hours}h ${minutes}m ${seconds}s left`;
      } else if (minutes > 0) {
        timeLeft = `${minutes}m ${seconds}s left`;
      } else {
        timeLeft = `${seconds}s left`;
      }

      badge.textContent = timeLeft;
      registerBtn.textContent = "Register Now";
      badge.style.display = "inline-block";
      registerBtn.disabled = false;
      registerBtn.style.opacity = "1";
      registerBtn.style.pointerEvents = "auto";
    } else {
      // Past event
      registerBtn.textContent = "Event Ended";
      registerBtn.disabled = true;
      registerBtn.style.opacity = "0.6";
      registerBtn.style.pointerEvents = "none";
      badge.style.display = "none";
    }
  });
}

function showCurrentUKTime() {
  const ukTime = new Date();
  const formatter = new Intl.DateTimeFormat('en-GB', {
    timeZone: 'Europe/London',
    hour12: false,
    hour: '2-digit',
    minute: '2-digit',
    second: '2-digit'
  });

  // Determine if we're in BST
  const isBST = formatter.resolvedOptions().timeZone === 'Europe/London' &&
    new Date().getTimezoneOffset() !== 0;

  // Update clock display
  const clockDisplay = document.querySelector('.clock-display');
  const timezoneDisplay = document.querySelector('.timezone-display');

  if (clockDisplay && timezoneDisplay) {
    clockDisplay.textContent = formatter.format(ukTime);
    timezoneDisplay.textContent = isBST ? 'British Summer Time (BST)' : 'Greenwich Mean Time (GMT)';
  }

  // Keep the existing console log
  console.log("Current UK Time:", formatter.format(ukTime));
}

