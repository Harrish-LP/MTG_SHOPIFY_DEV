  .unique-container {
    height: 100%;
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    /* Two columns per row */
    gap: 1em;
    padding: 1rem;
    max-width: 80em;
    margin: 0 auto;
  }

  .unique-card {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    width: 100%;
    height: 12em;
    padding: 2em;
    background: #FFF;
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1), 0 8px 24px rgba(0, 0, 0, 0.15);
    transition: all .35s ease;
    text-align: center;
    flex-direction: column;
    overflow: hidden;
    border-radius: 8px;
    transform-style: preserve-3d;
  }

  .unique-card::before,
  .unique-card::after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background: #000;
    height: 4px;
  }

  .unique-card::before {
    width: 0;
    opacity: 0;
    transition: opacity 0 ease, width 0 ease;
    transition-delay: .5s;
  }

  .unique-card::after {
    width: 100%;
    background: white;
    transition: width .5s ease;
  }

  .unique-card .content {
    width: 100%;
  }

  .unique-card h6 {
    color: #333;
    font-weight: normal;
    text-transform: normal;
    margin: 0 0 0.5em;
    letter-spacing: 1px;
    font-size: 1.5em;
  }

  .unique-card .hover_content {
    overflow: hidden;
    max-height: 0;
    transition: max-height .5s ease-in-out;
  }

  .unique-card .hover_content p {
    margin: 1em 0 0;
    color: #6E6E70;
    line-height: 1.4em;
  }

  .unique-card:hover {
    box-shadow: 0 15px 35px rgba(0, 0, 0, 0.2), 0 30px 60px rgba(0, 0, 0, 0.3);
    transform: translateY(-10px);
  }

  .unique-card:hover::before {
    width: 100%;
    opacity: 1;
    transition: opacity .5s ease, width .5s ease;
    transition-delay: 0;
  }

  .unique-card:hover::after {
    width: 0;
    opacity: 0;
    transition: width 0 ease;
  }

  .unique-card:hover .hover_content {
    max-height: 6em;
  }

  /* Media query for mobile screens */
  @media (max-width: 767px) {
    .unique-container {
      grid-template-columns: 1fr;
      /* Stack cards in a single column on mobile */
    }
  }
